{
  "name": "fcat-sensor-server",
  "version": "1.0.0",
  "description": "server for fcat sensor project",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node index.ts",
    "dev": "concurrently \"npm run devConcurrent\" \"npm run client\"",
    "client": "cd ../fcat-sensor-client && npm start",
    "devServer": "nodemon --watch . --ext ts --exec ts-node index.ts",
    "serial": "nodemon --watch . --ext ts --exec ts-node serial.ts",
    "devSerial": "nodemon --watch . --ext ts --exec ts-node serial.ts",
    "devConcurrent": "concurrently \"npm run devServer\" \"npm run devSerial\"",
    "serve": "IS_PROD=true ts-node index.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/uuid": "^9.0.7",
    "@types/waterline": "^0.13.8",
    "concurrently": "^8.2.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@serialport/parser-delimiter": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mathjs": "^12.4.1",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "serialport": "^12.0.0",
    "uuid": "^9.0.1",
    "waterline": "^0.15.2"
  }
}